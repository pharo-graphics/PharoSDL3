"
Example fragment shader.

Adapted from: https://www.shadertoy.com/view/4Xc3DM (free)
"
Class {
	#name : 'SDL3WigglesShaderSource',
	#superclass : 'SDL3ShaderSource',
	#category : 'SDL3-Tests-Shaders',
	#package : 'SDL3-Tests',
	#tag : 'Shaders'
}

{ #category : 'accessing' }
SDL3WigglesShaderSource class >> hlslSource [
	<script: 'self generateEmbeddedSourcesFromHLSL'>

	^ '
Texture2D iChannel0 : register(t0, space2);
SamplerState iChannel0Sampler : register(s0, space2);

cbuffer Params : register(b0, space3)
{
    float iTime;
    float2 iResolution;
}

// constants
static const float speed = 4.0;
static const float strength = 40.0;
static const float distortion = 0.03;

struct PSInput
{
    float4 color : COLOR0;
    float2 uv : TEXCOORD0;
};

float4 main(PSInput input) : SV_Target
{
    // 1. Base UV from input (Equivalent to fragCoord/iResolution.xy)
    float2 uv = input.uv;
    
    // 2. Reconstruct "p" (the centered coordinate space)
    // Corresponds to: (2. * fragCoord - iResolution.xy)
    // Derived as: iResolution * (2.0 * uv - 1.0)
    float2 p = iResolution * (2.0 * uv - 1.0);

    // 3. Calculate leg factors (Vignette/Masking based on aspect ratio)
    float legsx = 1.0 - length(p / iResolution.x);
    float legsy = 1.0 - length(p / iResolution.y);
    
    // 4. Calculate offsets
    float xOffset = sin((iTime * speed) + uv.y * strength) * (distortion * legsx);
    float yOffset = sin((iTime * speed) + uv.x * strength) * (distortion * legsy);
    
    // 5. Sample texture
    float4 tex = iChannel0.Sample(iChannel0Sampler, uv + float2(xOffset, yOffset));
    
    // 6. Return result
    return tex * input.color;
}
'
]

