"
I represent SDL3 shader example sources, extracted from SDL/test/testgpurender_effects.
"
Class {
	#name : 'SDL3CRTEffectShaderSource',
	#superclass : 'SDL3ShaderSource',
	#classVars : [
		'DXIL',
		'MSL',
		'SPIRV'
	],
	#category : 'SDL3-Tests-Shaders',
	#package : 'SDL3-Tests',
	#tag : 'Shaders'
}

{ #category : 'accessing' }
SDL3CRTEffectShaderSource class >> allSources [

	^ { MSL. DXIL. SPIRV }
]

{ #category : 'examples' }
SDL3CRTEffectShaderSource class >> initialize [

	SessionManager default registerSystemClassNamed: self name.

	self initializeSources
]

{ #category : 'examples' }
SDL3CRTEffectShaderSource class >> initializeDXIL [

	"CRT effect shader, in DXIL format. Extracted from SDL/test/testgpurender_effects_CRT.frag.dxil.h"
	DXIL :=
		self 
			format: SDL3GPUShaderFormat dxil
			sourceByteArray:  '' base64Decoded
]

{ #category : 'examples' }
SDL3CRTEffectShaderSource class >> initializeMSL [

	"CRT effect shader, in MSL format. Extracted from SDL/test/testgpurender_effects_CRT.frag.msl.h"
	MSL :=
		self
			format: SDL3GPUShaderFormat msl
			sourceByteArray: '' base64Decoded
]

{ #category : 'examples' }
SDL3CRTEffectShaderSource class >> initializeSPIRV [

	"CRT effect shader, in SPIRV format. Extracted from SDL/test/testgpurender_effects_CRT.frag.spv.h"
	SPIRV :=
		self
			format: SDL3GPUShaderFormat spirv
			sourceByteArray: '' base64Decoded
]

{ #category : 'examples' }
SDL3CRTEffectShaderSource class >> initializeSources [

	"CRT effect shader, in MSL format. Extracted from SDL/test/testgpurender_effects"
	self
		initializeMSL;
		initializeDXIL;
		initializeSPIRV
]

{ #category : 'accessing' }
SDL3CRTEffectShaderSource class >> numberOfSamplers [

	^ 1
]

{ #category : 'accessing' }
SDL3CRTEffectShaderSource class >> numberOfUniformBuffers [

	^ 1
]

{ #category : 'accessing' }
SDL3CRTEffectShaderSource class >> shaderName [

	^ 'CRT'
]

{ #category : 'system startup' }
SDL3CRTEffectShaderSource class >> startUp: isImageStarting [

	isImageStarting ifFalse: [ ^ self ].

	self initializeSources
]
