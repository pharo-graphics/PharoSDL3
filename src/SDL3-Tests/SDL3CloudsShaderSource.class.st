"
Post-processing fragment shader inspired on https://www.shadertoy.com/view/4tdSWr
"
Class {
	#name : 'SDL3CloudsShaderSource',
	#superclass : 'SDL3GPURenderStateDemoShaderSource',
	#category : 'SDL3-Tests-Shaders',
	#package : 'SDL3-Tests',
	#tag : 'Shaders'
}

{ #category : 'accessing' }
SDL3CloudsShaderSource class >> hlslSource [
	<script: 'self generateEmbeddedSourcesFromHLSL'>

	^ '
Texture2D inputTexture : register(t0, space2);
SamplerState inputTextureSampler : register(s0, space2);

cbuffer Context : register(b0, space3)
{
    float time; // Elapsed time in seconds
    float2 resolution; // Viewport width/height in pixels
}


// --- Helper Functions & Constants ---

static const float cloudscale = 1.1;
static const float speed = 0.03;
static const float clouddark = 0.5;
static const float cloudlight = 0.3;
static const float cloudcover = 0.2;
static const float cloudalpha = 8.0;
static const float skytint = 0.5;
static const float3 skycolour1 = float3(0.2, 0.4, 0.6);
static const float3 skycolour2 = float3(0.4, 0.7, 1.0);

static const float2x2 m = float2x2(1.6, 1.2, -1.2, 1.6);

float2 hash(float2 p)
{
    p = float2(dot(p, float2(127.1, 311.7)), dot(p, float2(269.5, 183.3)));
    return -1.0 + 2.0 * frac(sin(p) * 43758.5453123);
}

float noise(float2 p)
{
    const float K1 = 0.366025404; // (sqrt(3)-1)/2;
    const float K2 = 0.211324865; // (3-sqrt(3))/6;
    
    float2 i = floor(p + (p.x + p.y) * K1);
    float2 a = p - i + (i.x + i.y) * K2;
    // HLSL ternary vector selection
    float2 o = (a.x > a.y) ? float2(1.0, 0.0) : float2(0.0, 1.0);
    
    float2 b = a - o + K2;
    float2 c = a - 1.0 + 2.0 * K2;
    
    float3 h = max(0.5 - float3(dot(a, a), dot(b, b), dot(c, c)), 0.0);
    
    float3 n = h * h * h * h * float3(dot(a, hash(i + 0.0)), dot(b, hash(i + o)), dot(c, hash(i + 1.0)));
    
    // In HLSL float3(70.0) constructs a vector where all components are 70.0
    return dot(n, float3(70.0, 70.0, 70.0));
}

float fbm(float2 n)
{
    float total = 0.0, amplitude = 0.1;
    for (int i = 0; i < 7; i++)
    {
        total += noise(n) * amplitude;
        // HLSL matrix multiplication
        n = mul(n, m);
        amplitude *= 0.4;
    }
    return total;
}

// --- Main Shader ---

struct PSInput
{
    float4 color : COLOR0;
    float2 uv : TEXCOORD0;
};

float4 main(PSInput input) : SV_Target
{
    // Map input UV to p (0 to 1)
    float2 p = input.uv;
    
    // Correct Aspect Ratio using the resolution uniform
    float2 uv = p * float2(resolution.x / resolution.y, 1.0);
    
    float timeVal = time * speed;
    float q = fbm(uv * cloudscale * 0.5);
    
    // Ridged noise shape
    float r = 0.0;
    uv *= cloudscale;
    uv -= q - timeVal;
    float weight = 0.8;
    
    for (int i = 0; i < 8; i++)
    {
        r += abs(weight * noise(uv));
        uv = mul(uv, m) + timeVal;
        weight *= 0.7;
    }
    
    // Noise shape
    float f = 0.0;
    // Recalculate UV for next pass
    uv = p * float2(resolution.x / resolution.y, 1.0);
    uv *= cloudscale;
    uv -= q - timeVal;
    weight = 0.7;
    
    for (int j = 0; j < 8; j++)
    {
        f += weight * noise(uv);
        uv = mul(uv, m) + timeVal;
        weight *= 0.6;
    }
    
    f *= r + f;
    
    // Noise colour
    float c = 0.0;
    timeVal = time * speed * 2.0;
    uv = p * float2(resolution.x / resolution.y, 1.0);
    uv *= cloudscale * 2.0;
    uv -= q - timeVal;
    weight = 0.4;
    
    for (int k = 0; k < 7; k++)
    {
        c += weight * noise(uv);
        uv = mul(uv, m) + timeVal;
        weight *= 0.6;
    }
    
    // Noise ridge colour
    float c1 = 0.0;
    timeVal = time * speed * 3.0;
    uv = p * float2(resolution.x / resolution.y, 1.0);
    uv *= cloudscale * 3.0;
    uv -= q - timeVal;
    weight = 0.4;
    
    for (int l = 0; l < 7; l++)
    {
        c1 += abs(weight * noise(uv));
        uv = mul(uv, m) + timeVal;
        weight *= 0.6;
    }
    
    c += c1;
    
    // HLSL mix -> lerp
    float3 skycolour = lerp(skycolour2, skycolour1, p.y);
    float3 cloudcolour = float3(1.1, 1.1, 0.9) * clamp((clouddark + cloudlight * c), 0.0, 1.0);
   
    f = cloudcover + cloudalpha * f * r;
    
    float3 result = lerp(skycolour, clamp(skytint * skycolour + cloudcolour, 0.0, 1.0), clamp(f + c, 0.0, 1.0));
    
    //return float4(result, 1.0) * input.color;
    return lerp(float4(result, 1.0) * input.color, (inputTexture.Sample(inputTextureSampler, input.uv) * input.color), 0.5);

}'
]

